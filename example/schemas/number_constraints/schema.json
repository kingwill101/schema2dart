{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/number-constraints",
  "title": "Number Constraints Example",
  "description": "Demonstrates numeric validation keywords",
  "type": "object",
  "properties": {
    "minimum": {
      "type": "number",
      "minimum": 0,
      "description": "Must be >= 0"
    },
    "maximum": {
      "type": "number",
      "maximum": 100,
      "description": "Must be <= 100"
    },
    "exclusiveMinimum": {
      "type": "number",
      "exclusiveMinimum": 0,
      "description": "Must be > 0"
    },
    "exclusiveMaximum": {
      "type": "number",
      "exclusiveMaximum": 100,
      "description": "Must be < 100"
    },
    "multipleOf": {
      "type": "number",
      "multipleOf": 5,
      "description": "Must be multiple of 5"
    },
    "integerType": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999,
      "description": "Integer between 1 and 999"
    }
  }
}
